<Window x:Class="Yatzy.Queueing"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Yatzy"
        mc:Ignorable="d"
        Title="Queueing" Height="450" Width="800" SizeToContent="WidthAndHeight">
    <Grid Margin="10">
        <StackPanel>
            
            <TextBlock Text="Queueing..." FontSize="24" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            <Button Content="Cancel" FontSize="24" FontWeight="Bold" Click="Button_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
